<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <pipelines>
      <initialize>
        <processor type="Sitecore.Support.InitializerQueryBuilder, Sitecore.Support.193751" patch:after="processor[@type='Sitecore.Pipelines.Loader.RegisterjQuery, Sitecore.Kernel']" />
      </initialize>
    </pipelines>
    <contentSearch>
      <indexConfigurations>
        <defaultSolrIndexConfiguration>
          <indexFieldStorageValueFormatter>
            <patch:attribute name="type">
              Sitecore.Support.ContentSearch.SolrProvider.Converters.SolrIndexFieldStorageValueFormatter, Sitecore.Support.193751
            </patch:attribute>
          </indexFieldStorageValueFormatter>
        </defaultSolrIndexConfiguration>
      </indexConfigurations>
    </contentSearch>
  </sitecore>
</configuration>